; dial history (jrhb)
(let* ((range (lambda (s l r) (LinLin s 0 1 l r)))
       (mce-mrg (lambda (u) (Mrg (mceChannels u))))
       (mfv (transpose (list (list 697 770 852 941)
                             (list 1209 1336 1477 1633))))
       (numbers (transpose (list (list 3 0 0 0 1 1 1 2 2 2)
                                 (list 1 0 1 2 0 1 2 0 1 2))))
       (n (Dwhite dinf 7 12))
       (w (Dwhite 1 2 7))
       (b (Dbrown n 0.1 0.2 0.01))
       (rt (Dseq dinf (Mce2 w b)))
       (q (Dseq dinf (enumFromTo 1 10)))
       (g1 (GrayNoise))
       (g2 (GrayNoise))
       (d (LFDNoise3 0.5))
       (tr (Trig (TDuty rt 0 doNothing q 1) 0.09))
       (pat (Latch tr tr))
       (x (MouseX 0 1 linear 0.2))
       (h (Hasher (Mul pat x)))
       (which (Trunc (range h 0 (length numbers)) 1))
       (both (kr: (Select which numbers)))
       (dial (kr: (Select both mfv)))
       (sig (Mul3 (SinOsc dial 0) 0.05 tr))
       (dsig (DelayN sig 0.2 (range d 0 0.01)))
       (hiss (MulAdd g1 0.01 (HPF (Mul g2 0.02) 3000))))
  (Add dsig hiss))
