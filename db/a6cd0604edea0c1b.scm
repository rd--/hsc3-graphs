       (n3 (mceFill 2 (lambda (_) (BrownNoise kr))))
       (t (Dust kr 0.75))
       (l (Latch t t))
       (p (Mul (Pulse ar (wrp n1 2 (Mce2 11 15)) 0.01) 0.05))
       (f (wrp n2 300 1800))
       (rq (wrp n3 0.25 2)))
