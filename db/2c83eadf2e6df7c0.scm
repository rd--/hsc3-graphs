(let* ((n 11)
       (pkt (lambda (i)
              (let* ((a (Mul3 (LFSaw ar (AddMul i 1 5) 0) 0.5 pi))
                     (b (Mul (SinOsc ar (AddMul i 1 0.001) 0) 0.5))
                     (c (Max (Mul (LFSaw ar (Add 0.2 b) (FDiv i n)) 0.4) 0))
                     (d (MulAdd (SinOsc ar 0.03 (Add i 1)) 0.5 1))
                     (e (Mul (SinOsc ar (Add 200 i) 0) d))
                     (f (MulAdd (SinOsc ar 0.04 (Add i 2)) 0.5 1))
                     (g (Mul (SinOsc ar (Add 400 i) 0) f))
                     (h (MulAdd (SinOsc ar 0.05 (Add i 3)) 0.5 1))
                     (j (Mul (SinOsc ar (Add 800 i) 0) h))
                     (k (LinExp i 0 (Sub n 1) 70 1500))
                     (z (Mul5 (SinOsc ar k a) c e g j))
                     (l (LinLin i 0 (Sub n 1) -0.925 0.925)))
                (Pan2 z l (Recip n)))))
       (y (Limiter (LeakDC (mixFill n pkt) 0.995) 1 0.01)))
  (Mul (GVerb y 3 5 0.2 0.8 20 0.1 0.7 0.5 300) 0.2))
