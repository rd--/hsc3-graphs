; berlin 1977 (jmcc) #4
(let* ((sequ (lambda (s tr) (Demand tr 0 (Dseq dinf s))))
       (sequR (lambda (s tr) (Demand tr 0 (Dshuf dinf s))))
       (clockRate (MouseX 5 20 exponential 0.2))
       (clockTime (Fdiv 1 clockRate))
       (clock (Impulse clockRate 0))
       (patternList (List 55 60 63 62 60 67 63 58))
       (note (sequ patternList clock))
       (clock16 (PulseDivider clock 16 0))
       (note* (Add (sequR (list -12 -7 -5 0 2 5) clock16) note))
       (freq (MidiCps note*))
       (env (Decay2 clock (Mul 0.05 clockTime) (Mul 2 clockTime)))
       (amp (MulAdd env 0.1 0.02))
       (filt (MulAdd env (Mul (FSinOsc 0.17 0) 800) 1400))
       (pw (MulAdd (SinOsc 0.08 (Mce2 0 halfPi)) 0.45 0.5))
       (s (Mul (Pulse freq pw) amp)))
  (CombN (RLPF s filt 0.15) 0.2 (Mce2 0.2 0.17) 1.5))
