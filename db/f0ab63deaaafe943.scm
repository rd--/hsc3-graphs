(let* ((d0 (LPF (Dust2 ar (toLinRange (LFNoise1 kr 0.2) 40 50)) 7000))
       (n0 (MulAdd (PinkNoise ar) (MulAdd (LFNoise1 kr 0.3) 0.02 0.08) d0))
       (e0 (Line kr 0 1 10 0))
       (p0 (TanH (Mul3 3 (GVerb (HPF n0 400) 250 100 0.25 0.5 15 0.3 0.7 0.5 300) e0)))
       (lfNoise1C (lambda (r f) (Clip (LFNoise1 r f) 0 1)))
       (n1 (Mul (PinkNoise ar) (Pow (Mul (lfNoise1C kr 3) (lfNoise1C kr 2)) 1.8)))
       (f1 (LinExp (LFNoise1 kr 1) -1 1 100 2500))
       (e1 (Line kr 0 0.7 30 0))
       (p1 (Mul (GVerb (TanH (LPF (Mul 10 (HPF n1 20)) f1)) 270 30 0.7 0.5 15 0.5 0.7 0.5 300) e1)))
 (Mul (Limiter (Add p0 p1) 1 0.01) 0.2))
