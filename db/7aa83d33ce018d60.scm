; shifting pulses (rd)
(let* ((wrp (lambda (i l r) (LinLin i -1 1 l r)))
       (n1 (mceFill 2 (lambda (_) (BrownNoise))))
       (n2 (mceFill 2 (lambda (_) (BrownNoise))))
       (n3 (mceFill 2 (lambda (_) (BrownNoise))))
       (t (Dust 0.75))
       (l (Latch t t))
       (p (Mul (Pulse (wrp n1 2 (Mce2 11 15)) 0.01) 0.05))
       (f (wrp n2 300 1800))
       (rq (wrp n3 0.25 2)))
  (Mrg2
   (Add
    (Mul (Formant (Mce2 20 21) (wrp (LFNoise2 2) 10 100) 200) 0.15)
    (Mul l (RLPF p f rq)))
   (SendTrig t 0 t)))
