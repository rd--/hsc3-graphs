; eggcrate (rd)
(let* ((eggcrate-f (lambda (u v) (Mul (Cos (Mul u pi)) (Sin (Mul v pi)))))
       (p (make-mce (list 64 72 96 128 256 6400 7200 8400 9600)))
       (x (BrownNoise kr))
       (y (BrownNoise kr))
       (t (Dust kr 2.4))
       (f0 (t-choose t p))
       (f1 (t-choose t p))
       (f (lin-lin (eggcrate-f x y) -1 1 f0 f1))
       (a (lin-lin x -1 1 0 0.1)))
  (Pan2 (mix (SinOsc ar f 0)) y a))
